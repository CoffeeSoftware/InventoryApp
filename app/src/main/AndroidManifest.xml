<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comcoffeesoftware.httpsgithub.inventarsoft">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_barcode_scan_black_48dp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_barcode_scan_black_48dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListaProduseActivity" />
        <activity android:name=".EditorActivity" />
        <activity android:name=".ContactActivity" />
        <activity android:name=".ScanActivity" />

        <provider
            android:name=".ProdusProvider"
            android:authorities="comcoffeesoftware.httpsgithub.inventarsoft"
            android:exported="false" />

        <activity android:name=".HelpActivity"></activity>
    </application>

</manifest>